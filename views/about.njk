{%extends "layout.njk"%} 


{%block head%}


    <title>Sobre - Joabe Chaves</title>

{% endblock %} 

 

{% block content%}
    <div id="wraper">
        <img src="{{about.avatar}}" alt="imagem de {{about.name}}">
        <h1>{{about.name}}</h1>
        <h2>{{about.role}}</h2>
        <p>{{about.description}}</p>
    </div>
    <div id="linksFooter" class="links">
        {% for links in about.link %}
        <a href="{{links.url}}" target="_blank">{{links.name}}</a>
        {% endfor %}
    </div>
{% endblock %}